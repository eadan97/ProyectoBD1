@model IEnumerable<Proyecto.Models.AlineacionPartido>

@{
    ViewBag.Title = "Alineacion en partido";
}

<h2>Alineacion en partido</h2>

<p>
    @Html.ActionLink("Crear nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Partido.codPartido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Jugador.FuncionarioDeportivo.Persona.nbrPersona)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Equipo.nombreEquipo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.estado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.usuarioCreador)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.usuarioModificador)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fechaCreacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fechaModificacion)
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Partido.codPartido)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Jugador.FuncionarioDeportivo.Persona.nbrPersona)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Equipo.nombreEquipo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.estado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fechaCreacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fechaModificacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.usuarioCreador)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.usuarioModificador)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new {item.codPartido, item.codPersona}) |
            @Html.ActionLink("Detalles", "Details", new { item.codPartido, item.codPersona }) |
            @Html.ActionLink("Eliminar", "Delete", new { item.codPartido, item.codPersona })
        </td>
    </tr>
}

</table>
