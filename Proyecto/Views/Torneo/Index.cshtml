@model IEnumerable<Proyecto.Models.Torneo>

@{
    ViewBag.Title = "Torneo";
}

<h2>Torneo</h2>

<p>
    @Html.ActionLink("Crear nuevo", "Create")
</p> 
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Competicion.nbrCompeticion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.codTemporada)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.usuarioCreador))
        </th>
        <th>
            @Html.DisplayNameFor(model => model.usuarioModificador)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fechaCreacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fechaModificacion)
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Competicion.nbrCompeticion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.codTemporada)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.usuarioCreador)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.usuarioModificador)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fechaCreacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fechaModificacion)
        </td>

        <td>
            @Html.ActionLink("Editar", "Edit", new {id = item.codTorneo}) |
            @Html.ActionLink("Detalles", "Details", new {id = item.codTorneo}) |
            @Html.ActionLink("Eliminar", "Delete", new {id = item.codTorneo})
        </td>
    </tr>
}

</table>
