@model IEnumerable<Proyecto.Models.DatosEntrenador_Result>

@{
    ViewBag.Title = "Arbitros en torneo";
}

<h2>Arbitros en torneo</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Equipo)
        </th>
        <th>
            Fecha de contratacion
        </th>
        <th>
            Fecha de baja
        </th>
        <th>
            Nombre de la competicion
        </th>
        <th>
            Tipo de la competicion
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Posicion)
        </th>
    </tr>

    @foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Equipo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_contratacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_baja)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre_competicion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tipo_competicion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Posicion)
        </td>
    </tr>
    }

</table>